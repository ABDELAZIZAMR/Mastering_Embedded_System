#include"stdio.h"
void main(){
	char alpha ;
	char a[12]= {'a','e','i','o','u','A','E','I','O','U'};
	printf("Enter an alphabet:");
	fflush(stdin);fflush(stdout);
	scanf("%c",&alpha);
	if(alpha == a){
		printf("%c is a vowel.",alpha);
	}
	 else (alpha != a){
		printf("%c is a consonant.",alpha);
	 }
}


