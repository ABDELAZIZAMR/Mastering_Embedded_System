#include "stdio.h"
void main(){
	char a ;
	printf("##########Console-output###\n");
	printf("Enter a character :");
	fflush(stdout);
	fflush(stdin);
	scanf("%c",&a);
	printf("ASCII value of %c = \n",&a);
	printf("###########################");


}

