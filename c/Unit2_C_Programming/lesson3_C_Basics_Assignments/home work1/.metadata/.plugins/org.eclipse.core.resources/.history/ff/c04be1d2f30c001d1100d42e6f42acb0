#include"stdio.h"
void main()
{
	int a ,b,sum;
	printf("##########Console-output###\n");
	printf("Enter two integer: ");
	fflush(stdout);
	fflush(stdin);
	scanf("%d",&a);
	scanf("%d",&b);
	sum=a+b;
	printf("sum :%d \n",sum);
	printf("###########################");

}
